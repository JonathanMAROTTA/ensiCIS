{
  "popups": {
    "welcome": ".close-dialog",
    "cookies": ".cc-btn"
  },
  "navbar": {
    "account": "#navbarAccount",
    "profile": "button.ng-tns-c122-2:nth-child(1)",
    "logout": "#navbarLogoutButton"
  },
  "profile": {
    "email": "#email"
  },
  "contact": {
    "comment": "#comment",
    "submit": "#submitButton",
    "captcha": {
      "question": "#captcha",
      "controller": "#captchaControl"
    },
    "rating": {
      "controller": "#rating",
      "attributes": {
        "min": "aria-valuemin",
        "max": "aria-valuemax"
      }
