    "securityOption": "mat-option",
    "securityAnswer": "#securityAnswerControl",
    "submit": "#registerButton"
  },
  "login": {
    "email": "#email",
    "password": "#password",
    "submit": "#loginButton"
  },
  "complain": {
    "message": "#complaintMessage",
    "fileUpload": "input[type=\"file\"]",
    "submit": "#submitButton"
  },
  "webSandbox": {
    "webIDE": ".CodeMirror"
  },
  "scoreBoard": {
    "anyGroup": ".score-group"
  }
